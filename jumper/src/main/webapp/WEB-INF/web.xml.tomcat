  </servlet-mapping>

  <servlet>
    <servlet-name>oka</servlet-name>
    <servlet-class>com.jack.pinpoint.jumper.OkAsyncServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>oka</servlet-name>
    <url-pattern>/oka</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ggs</servlet-name>
