    <url-pattern>/hc4s</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>hc4a</servlet-name>
    <servlet-class>com.jack.pinpoint.jumper.HttpClient4AsyncServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>hc4a</servlet-name>
    <url-pattern>/hc4a</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>hc3</servlet-name>
    <servlet-class>com.jack.pinpoint.jumper.HttpClient3Servlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>hc3</servlet-name>
    <url-pattern>/hc3</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ning</servlet-name>
    <servlet-class>com.jack.pinpoint.jumper.NingServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ning</servlet-name>
    <url-pattern>/ning</url-pattern>
  </servlet-mapping>

  <servlet>
