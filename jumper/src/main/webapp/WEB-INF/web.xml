<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
                 version="2.5">
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>httpclient</servlet-name>
    <servlet-class>com.jack.pinpoint.jumper.HttpClientServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>httpclient</servlet-name>
    <url-pattern>/httpclient</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>jdkhttp</servlet-name>
    <servlet-class>com.jack.pinpoint.jumper.JDKHttpServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>jdkhttp</servlet-name>
    <url-pattern>/jdkhttp</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>hystrix</servlet-name>
    <servlet-class>com.jack.pinpoint.jumper.HystrixServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>hystrix</servlet-name>
    <url-pattern>/hystrix</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>thrift</servlet-name>
    <servlet-class>com.jack.pinpoint.jumper.ThriftServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>thrift</servlet-name>
    <url-pattern>/thrift</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>async</servlet-name>
    <servlet-class>com.jack.pinpoint.jumper.AsyncServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>async</servlet-name>
    <url-pattern>/async</url-pattern>
  </servlet-mapping>

</web-app>
